{
    "name": "booking-plugin/booking-plugin",
    "description": "WordPress booking plugin",
    "type": "wordpress-plugin",
    "require": {
        "firebase/php-jwt": "6.10.0"
    },
    "autoload": {
        "psr-4": {
            "BookingPlugin\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "yoast/phpunit-polyfills": "^2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "BookingPlugin\\Tests\\": "tests/"
        }
    }
}
